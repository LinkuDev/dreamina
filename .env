
# Simple Multi-Instance Config for Dreamina
# Cấu hình đơn giản cho việc chạy song song

# Số lượng instances muốn chạy
INSTANCES=3

# Thư mục cookies (sẽ auto tạo cookies1, cookies2, cookies3...)
COOKIES_BASE=cookies

# File prompts (sẽ auto tạo prompts1.txt, prompts2.txt, prompts3.txt...)
PROMPTS_BASE=prompts

# Thư mục output (sẽ auto tạo outputs1, outputs2, outputs3...)
OUTPUTS_BASE=outputs

# Browser settings
BROWSER_HEADLESS=false
BROWSER_TIMEOUT=30
BROWSER_WAIT_TIME=30

# Target Configuration  
TARGET_URL=https://dreamina.capcut.com/ai-tool/generate?type=image
DREAMINA_HOME_URL=https://dreamina.capcut.com/ai-tool/home
DREAMINA_CREATIONS_URL=https://dreamina.capcut.com/ai-tool/asset

# API Configuration
API_BASE_URL=http://localhost:8000/v1

# Generation Settings
ASPECT_RATIO=16:9,1:1,9:16
IMAGE_COUNT=4

# Delay giữa các instances (seconds)
STARTUP_DELAY=5

# Legacy settings (for single instance)
COOKIES_FOLDER=cookies
PROMPT_FILE=prompts/1.txt

# Advanced Settings
MAX_RETRIES=3
NAVIGATION_DELAY=3000
MODAL_TIMEOUT=3000
AUTH_CHECK_DELAY=3000

# Debug Settings
VERBOSE_LOGGING=true
SCREENSHOT_ON_ERROR=true
SAVE_PAGE_CONTENT=false
